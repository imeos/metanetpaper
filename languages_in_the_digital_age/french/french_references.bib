@Misc{terminology_table,
   note = {  {\bf Reconnaissance de la parole}~: Commande vocale, dictée vocale, transcription d{\mbox '}émissions, transcription de conversations, dialogue oral.\\
  {\bf Synthèse vocale}~: Synthèse \a` partir du texte, génération de parole.\\
  {\bf Analyse grammaticale}~: Tokenization, analyse morpho-syntaxique, analyse syntaxique de surface ou profonde.\\
  {\bf Analyse sémantique}~: (Sémantique des phrases (WSD, structure argumentaire, rôles sémantiques), Sémantique de textes (résolution de coréférences, contexte, pragmatique, inférence), Traitement avancé du discours (structure des textes, cohérence, structure rhétorique/RST, zonage argumentatif, argumentation, patterns textuels, types de textes etc.\\
  {\bf Génération}~: Génération de phrases, génération de rapports, génération de textes.\\
  {\bf Traduction Automatique}~: et traduction vocale\\
  {\bf Corpus de textes}~: Corpus de référence, Corpus syntaxiques (corpus arborés)\\
  {\bf Corpus de parole}~: brut, étiqueté/annoté, dialogue\\
  {\bf Ressources lexicales}~: Lexiques, Bases terminologiques, Thesauri, WordNets
}}


@Misc{terminology_table_eng,
    note = {  {\bf Speech Recognition}: voice command, voice dictation, broadcast transcription, conversational speech transcription, oral dialog\\
  {\bf Speech Synthesis}: text-to-speech synthesis, speech generation\\
  {\bf Grammatical analysis}: tokenization, POS tagging, morphological analysis/generation, shallow or deep syntactic analysis\\
  {\bf Semantic analysis}:sentence semantics (WSD, argument structure, semantic roles, text semantics (coreference resolution, context, pragmatics, inference), Advanced Discourse Processing (text structure, coherence, rhetorical structure/RST, argumentative zoning, argumentation, text patterns, text types etc.\\
  {\bf Text Generation}: sentence generation, report generation, text generation.\\
  {\bf Machine Translation}: and speech translation\\
  {\bf Text Corpora}~: Reference Corpora, Syntax corpora (treebanks, dependency banks)\\
  {\bf Speech Corpora}~: raw speech data, labeled/annotated speech data, spoken dialogue data
  {\bf Lexical Resources}~: Lexicons, Terminology databases, Thesauri, WordNets
     }
}

